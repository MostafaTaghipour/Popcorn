    "@react-navigation/stack": "5.3.1",
    "axios": "^0.19.2",
    "electron": "^6.0.12",
    "expo": "~37.0.9",
    "expo-asset": "~8.1.0",
    "expo-constants": "~9.0.0",
    "expo-font": "~8.1.0",
    "expo-linking": "^1.0.1",
    "expo-localization": "~8.1.0",
