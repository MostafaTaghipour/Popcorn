{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node scripts/set-environment.js development && expo start",
    "android": "node scripts/set-environment.js development && expo start --android",
    "android:staging": "node scripts/set-environment.js staging && expo start --android",
    "android:production": "node scripts/set-environment.js production && expo start --android",
    "ios": "node scripts/set-environment.js development && expo start --ios",
    "ios:staging": "node scripts/set-environment.js staging && expo start --ios",
    "ios:production": "node scripts/set-environment.js production && expo start --ios",
    "web": "node scripts/set-environment.js development && expo start --web",
    "web:staging": "node scripts/set-environment.js staging && expo start --web",
    "web:production": "node scripts/set-environment.js production && expo start --web",
    "electron": "yarn expo-electron start",
    "eject": "expo eject",
    "test": "jest --watchAll --silent",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf node_modules/&& yarn install"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|redux-persist-expo-securestore|@sentry/.*)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "~10.0.6",
