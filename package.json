    "@types/axios": "^0.14.0",
    "@types/i18n-js": "^3.0.3",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.9",
    "babel-preset-expo": "~8.1.0",
